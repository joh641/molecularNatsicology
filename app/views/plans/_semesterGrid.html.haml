#semesters
  %div(class="contentWrapper data-ng-cloak")
    .row 
      .col-md-1
        .semester-name#fall Fall
      .thumbnail.col-md-2(data-drop="true" data-ng-model="fall1" data-jqyoui-options="{accept:'.btn-draggable:not([data-ng-model=fall1])'}" jqyoui-droppable="{multiple:true}")
        .caption 
          .year 2013
          //%a.fa.fa-times-circle(data-ng-click="clearSemester('fall1')")
          %a.fa.fa-cog
          .btn.btn-info.btn-draggable(data-ng-click="courseDisplay(course.id)" data-ng-repeat="course in fall1" data-ng-show="course.name" data-drag="{{course.drag}}" data-jqyoui-options="{revert: 'invalid'}" data-ng-model="fall1" jqyoui-draggable="{onStart: 'startDragging', index: {{$index}},animate:true}")
            {{course.name}}
            = render 'plans/course_modal'
