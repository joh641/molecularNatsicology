#rules.col-md-2
  %h3 Rules
  %table.table
    %thead
      %tr
        %th Rule
        %th Description
        %th Result
        %th Fulfilling Set
    .body
      %tr(data-ng-repeat="rule in rules")
        %td 
          %a(href="{{rule.url}}") {{rule.name}}
        %td {{rule.description}}
        %td {{rule.result.pass}}
        %td {{rule.result.courses}}